{
  "name": "Tipa - Report System",
  "description": "Tipa - Backend",
  "version": "0.1.0",
  "engines": {
    "node": "16.x",
    "npm": "8.x",
    "yarn": "1.x"
  },
  "scripts": {
    "start": "nodemon --watch \"src/**/*.ts\" -e ts,json --exec \"ts-node --transpile-only ./src/server.ts\"",
    "test": "nodemon --exec \"cross-env NODE_ENV=test mocha -r ts-node/register ./src/**/*.test.ts --exit || exit 1\"",
    "build": "tsc",
    "aws-postbuild": "npm run build && cd frontend && npm install && npm run build"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.9",
    "@types/bcrypt": "^5.0.0",
    "@types/geoip-lite": "^1.4.1",
    "@types/lodash": "^4.14.191",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.0.0",
    "@types/object-hash": "^3.0.1",
    "@types/slug": "^5.0.3",
    "app-root-path": "^3.1.0",
    "axios": "0.21.1",
    "bcrypt": "5.0.1",
    "capitalize": "^2.0.4",
    "cli-highlight": "^2.1.11",
    "copyfiles": "^2.4.1",
    "cors": "2.8.5",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "express-rate-limit": "6.4.0",
    "form-data": "^4.0.0",
    "formidable-serverless": "1.1.1",
    "geoip-lite": "^1.4.6",
    "helmet": "5.1.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "^4.17.21",
    "mv": "2.1.1",
    "nodemon": "^2.0.16",
    "object-hash": "^3.0.0",
    "rrule": "^2.7.1",
    "slug": "^8.0.0",
    "ts-node": "^10.9.1"
  },
  "private": true,
  "devDependencies": {
    "cross-env": "7.0.3",
    "mocha": "10.0.0",
    "node-mocks-http": "1.11.0",
    "typescript": "4.7.2"
  }
}
